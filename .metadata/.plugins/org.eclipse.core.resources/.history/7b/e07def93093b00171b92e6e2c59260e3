<script>
/* IIFE
(function(){
	document.write("<p/>"+"Hello Ram");
})();
var f = function(){
	document.write("<p/>"+"Hello Ram");
}
f();
*/
function totalnum(){
	
	var total = 0;
	
	for(var i=0;i<arguments.length;i++){
		
		
		total+=arguments[i];
		
	}
	return total;
}
document.write(totalnum(10,20,30,40));
</script>

</head>
